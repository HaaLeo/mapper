    "@nestjs/testing": "~9.0.2",
    "@nrwl/devkit": "14.3.6",
    "@nrwl/eslint-plugin-nx": "14.3.6",
    "@nrwl/jest": "14.3.6",
    "@nrwl/js": "14.3.6",
    "@nrwl/linter": "14.3.6",
    "@nrwl/nest": "14.3.6",
    "@nrwl/nx-cloud": "14.1.3",
    "@nrwl/web": "14.3.6",
    "@nrwl/workspace": "14.3.6",
    "@release-it/bumper": "4.0.0",
